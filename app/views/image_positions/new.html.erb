<h1>New image_position</h1>

<% form_for(@image_position) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :latitude %><br />
    <%= f.text_field :latitude %>
  </p>
  <p>
    <%= f.label :latitudeNumber %><br />
    <%= f.text_field :latitudeNumber %>
  </p>
  <p>
    <%= f.label :longitude %><br />
    <%= f.text_field :longitude %>
  </p>
  <p>
    <%= f.label :longitudeNumber %><br />
    <%= f.text_field :longitudeNumber %>
  </p>
  <p>
    <%= f.label :altitude %><br />
    <%= f.text_field :altitude %>
  </p>
  <p>
    <%= f.label :orientation %><br />
    <%= f.text_field :orientation %>
  </p>
  <p>
    <%= f.label :tilt %><br />
    <%= f.text_field :tilt %>
  </p>
  <p>
    <%= f.label :roll %><br />
    <%= f.text_field :roll %>
  </p>
  <p>
    <%= f.label :accuracy %><br />
    <%= f.text_field :accuracy %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', image_positions_path %>