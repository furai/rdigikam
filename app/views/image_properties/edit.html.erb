<h1>Editing image_property</h1>

<% form_for(@image_property) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :property %><br />
    <%= f.text_field :property %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @image_property %> |
<%= link_to 'Back', image_properties_path %>